<script>
    function getQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return unescape(r[2]);
        return null;
    }
    var panoramaUrl = getQueryString('url');
    var houseId = getQueryString('id');
    console.log(panoramaUrl,houseId);
    if(houseId){

    }else{

    }
    var PSV = new PhotoSphereViewer({
        container: 'photosphere',
        panorama: panoramaUrl,//panos[0].url,
        // caption: panos[0].desc,
        //fisheye: true,
//    webgl: false,
//        min_fov:59,
//        max_fov:60,
        time_anim: 2000,
        default_long: Math.PI / 2,
        loading_img: 'assets/photosphere-logo.gif',
        navbar: [
            'autorotate', 'zoom', 'caption', 'gyroscope', 'stereo', 'fullscreen'
        ]
    });

    PSV.zoom(0);

</script>

